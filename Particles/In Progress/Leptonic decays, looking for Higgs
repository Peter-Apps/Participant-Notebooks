(data['px^2'] + data['py^2'] + data['pz^2'])\n","\n","\n","\n","\n","\n","data['mass'] = (np.sqrt((data['E']**2) - (data['ptotal']**2))) #This makes a new column called \"m1\" and fills it with E1 + p1 for each event\n","data = data.fillna(0) # fills in a zero for any imaginary values your caluclation might produce\n","\n","\n","\n","\n","\n","\n","plt.hist(data['mass'], bins=500, range=[0,400], log=False)  # makes the histogram\n","plt.title(\"Double muon decay production invariant masses\")\n","plt.xlabel(\"x-axis label (in GeV/c^2)\")\n","plt.ylabel(\"number of events\")\n","plt.grid(False);"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]}]}